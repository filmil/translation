#: src/getopt.c:693
#, c-format
msgid "%s: option `%s' is ambiguous\n"
msgstr "%s: Избор `%s' је двосмислен\n"

#: src/getopt.c:718
#, c-format
msgid "%s: option `--%s' doesn't allow an argument\n"
msgstr "%s: избор `--%s' не допушта аргументе\n"

#: src/getopt.c:723
#, c-format
msgid "%s: option `%c%s' doesn't allow an argument\n"
msgstr "%s: избор `%c%s' не допушта аргументе\n"

#: src/getopt.c:741 src/getopt.c:914
#, c-format
msgid "%s: option `%s' requires an argument\n"
msgstr "%s: избор `%s' захтева аргумент\n"

#: src/getopt.c:770
#, c-format
msgid "%s: unrecognized option `--%s'\n"
msgstr "%s: избор `--%s' није препознат\n"

#: src/getopt.c:774
#, c-format
msgid "%s: unrecognized option `%c%s'\n"
msgstr "%s: избор `%c%s' није препознат\n"

#: src/getopt.c:800
#, c-format
msgid "%s: illegal option -- %c\n"
msgstr "%s: неисправан избор -- %c\n"

#: src/getopt.c:803
#, c-format
msgid "%s: invalid option -- %c\n"
msgstr "%s: непостојећи избор -- %c\n"

#: src/getopt.c:833 src/getopt.c:963
#, c-format
msgid "%s: option requires an argument -- %c\n"
msgstr "%s: избор захтева аргумент -- %c\n"

#: src/getopt.c:880
#, c-format
msgid "%s: option `-W %s' is ambiguous\n"
msgstr "%s: избор `-W %s' је двосмислен\n"

#: src/getopt.c:898
#, c-format
msgid "%s: option `-W %s' doesn't allow an argument\n"
msgstr "%s: избор `-W %s' не дозвољава аргумент\n"

#: src/hello.c:164
msgid "Too many arguments\n"
msgstr "Сувише аргумената\n"

#: src/hello.c:165
#, c-format
msgid "Try `%s --help' for more information.\n"
msgstr "Куцајте `%s --help' за више помоћи.\n"

#. TRANSLATORS: --help output 1
#. no-wrap
#: src/hello.c:177
msgid "GNU hello, THE greeting printing program.\n"
msgstr "GNU hello, ЧУВЕНИ програм за штампање поздрава.\n"

#. TRANSLATORS: --help output 2
#. no-wrap
#: src/hello.c:182
#, c-format
msgid "Usage: %s [OPTION]\n"
msgstr "Употреба: %s [ОПЦИЈА]\n"

#. TRANSLATORS: --help output 3 : options 1/2
#. no-wrap
#: src/hello.c:188
msgid ""
"  -h, --help          display this help and exit\n"
"  -v, --version       display version information and exit\n"
msgstr ""
"  -h, --help          прикажи овај текст и заврши с радом\n"
"  -v, --version       прикажи ознаку верзије и заврши с радом\n"

#. TRANSLATORS: --help output 4 : options 2/2
#. no-wrap
#: src/hello.c:195
msgid ""
"  -t, --traditional       use traditional greeting format\n"
"  -n, --next-generation   use next-generation greeting format\n"
"  -m, --mail              print your mail\n"
msgstr ""
"  -t, --traditional       користи традиционални формат за поздрав\n"
"  -n, --next-generation   користи нови формат за поздрав\n"
"  -m, --mail              испиши приспелу пошту\n"

#. TRANSLATORS: --help output 5 (end)
#. TRANSLATORS, please don't forget to add the contact address for
#. your translation!
#. no-wrap
#: src/hello.c:205
#, c-format
msgid "Report bugs to <%s>.\n"
msgstr "Пријавите грешке на адресу <%s>.\n"

#: src/hello.c:217
#, c-format
msgid ""
"Copyright (C) %s Free Software Foundation, Inc.\n"
"There is NO warranty; not even for MERCHANTABILITY or FITNESS FOR A\n"
"PARTICULAR PURPOSE.  You may redistribute copies of GNU %s under the terms\n"
"of the GNU General Public License.\n"
"For more information about these matters, see the file named COPYING.\n"
msgstr ""
"Copyright (C) %s Free Software Foundation, Inc.\n"
"Не дају се НИКАКВЕ гаранције; ни за ПРОДАЈНУ ни за УПОТРЕБНУ вредност.\n"
"Можете делити копије GNU %s према условима ГНУ Опште јавне лиценце.\n"
"За више информација о правним детаљима погледајте датотеку COPYING.\n"

#: src/hello.c:228
#, c-format
msgid "%s: Incompatible flags: -m and -t\n"
msgstr "%s: Избори не могу да садејствују: -m и -t\n"

#: src/hello.c:260
#, c-format
msgid "%s: Who are you?\n"
msgstr "%s: Ко сте ви?\n"

#: src/hello.c:282
#, c-format
msgid "%s: Cannot find your mail spool file.\n"
msgstr "%s: Ваше поштанско сандуче није пронађено.\n"

#: src/hello.c:326
msgid "Nothing happens here."
msgstr "Ништа се не дешава."

#: src/hello.c:332
msgid "hello, world\n"
msgstr "здраво, свете\n"

